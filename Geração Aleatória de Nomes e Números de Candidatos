void gerar_candidatos(Candidato candidatos[], int total) {
    char *nomes[] = {"Carlos", "Ana", "Roberto", "Mariana", "Jo√£o"};
    int numeros[] = {10, 20, 30, 40, 50};

    for (int i = 0; i < total; i++) {
        strcpy(candidatos[i].nome, nomes[i]);
        candidatos[i].numero = numeros[i];
        candidatos[i].votos = 0;
    }
}
